{
  "name": "RPL",
  "version": "0.3.1",
  "description": "High-performance C++20 serialization/deserialization library for RoboMaster",
  "keywords": "robomaster, serialization, packet, protocol, c++20",
  "repository": {
    "type": "git",
    "url": "https://github.com/RoboMaster-DLMU-CONE/rpl.git"
  },
  "authors": [
    {
      "name": "RoboMaster-DLMU-CONE",
      "email": "moonfeather120@outlook.com",
      "url": "https://github.com/RoboMaster-DLMU-CONE"
    }
  ],
  "license": "ISC",
  "frameworks": [
    "arduino"
  ],
  "platforms": [
    "espressif32",
    "ststm32",
    "teensy",
    "raspberrypi"
  ],
  "export": {
    "include": "src"
  },
  "build": {
    "flags": [
      "-std=gnu++20",
      "-Wno-register"
    ],
    "unflags": [
      "-std=gnu++11",
      "-std=gnu++14",
      "-std=gnu++17"
    ]
  }
}
